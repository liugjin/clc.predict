a.btn.modal-trigger.standard-report-btn(href='#tasktime-select' style='margin-top: 0.6rem !important') {{parameters.str || '打印'}}

#tasktime-select.modal.modal-fixed-footer(md-modal, style="width: 500px;height: 200px;top: 35%;overflow: hidden;")
    .modal-content(style="padding: 12px;overflow: hidden;")
        div(style="font-size: 20px;font-weight: bold;") 日期选择
        div(style="padding: 32px 24px;")
            div(style="width:33%;float:left;")
                select.browser-default.time-select(ng-model="queryDay.year" ng-change="changeDays()")
                    option(value="") --请选择--
                    option(ng-repeat="(index, year) in options.year" value="{{year}}") {{year}}
                div(style="height: 28px;line-height: 1.8;width: 24px;float: left;") 年
            div(style="width:33%;float:left;")
                select.browser-default.time-select(ng-model="queryDay.month" ng-change="changeDays()")
                    option(value="") --请选择--
                    option(ng-repeat="(index2, month) in options.month" value="{{month}}") {{month}}
                div(style="height: 28px;line-height: 1.8;width: 24px;float: left;") 月
            div(style="width:33%;float:left;")
                select.browser-default.time-select(ng-model="queryDay.day")
                    option(value="") --请选择--
                    option(ng-repeat="(index3, day) in options.day" value="{{day}}") {{day}}
                div(style="height: 28px;line-height: 1.8;width: 24px;float: left;") 日
    .modal-footer(style="overflow: hidden;")
        a.btn(ng-click="setQuery(0)" style="position: relative; top: 6px;") 确认
        a.btn(style="margin: 0 12px;position: relative; top: 6px;" ng-click="setQuery(-1)") 取消