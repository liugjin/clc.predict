.card
  card-border(controller="vm" parameters='{ title: "故障运维概况", desc: ["故障运维单数（单）", count, "单"] }')
  .col.s12.l12.contents
    .col.s7.l7.row1
      bar-timeline(controller="vm" parameters="{ type: 1, series: warnSeries, data: warnData, date }") 
    .col.s5.l5.row2
      .col.s6(style="height: 100%;")
        div(ng-repeat="item in equipData" style="width: 50%; float: left; height: 100%")
          capacity-bar(controller="controller" parameters="{ key: item.key, val: item.val, max: item.max, titleLen: 5 }")
      .col.s6(style="height: 100%;")
        bar-vertical-stack2(controller="controller" parameters='{ data: rankData }')