.signal-content.center
    ul.content-ul
        //li.modal-trigger(data-target='stream-chart-modal' ng-click='checkStreamSignal()')
        li.modal-trigger(ng-repeat="data in streamArr" data-target='stream-chart-modal' ng-click='checkStreamSignal(data.model)')
            .signal-icon.center
                img(src="{{data.model.imgUrl}}" width="26px" height="26px")
            .signal-name.center
                span {{data.model.name}}

#stream-chart-modal.modal.modal-fixed-footer(md-modal,style="width: 1400px;height:800px;margin-top:100px;")
    .modal-content(style="height:100% !important;padding:0 !important;")
        .closepart(style="position:absolute;right:12px;top:22px;z-index:999;")
            div(style="width:30px;height:30px;line-height:30px;display:block;text-align:center;cursor:pointer;" ng-click="hidemodal()")
                span.white-text(style="font-size:22px;" ng-click="hidemodal()")
                    img(ng-src="{{getComponentPath('images/close.png')}}")
        .video(style="display:block;height:100%;width:100%" ng-if="originalDataFlag")
            //stream-shock-curve(ng-if='chartModal == 2' controller='vm',parameters='{station:station.model.station,equipment:equipment.model.equipment,streamFlag:streamFlag,currentSignal:currentSignal.signal,currentName:currentSignal.name}')
            stream-current-curve(ng-if='chartModal == 3' controller='vm',parameters='{station:station.model.station,equipment:equipment.model.equipment,streamFlag:streamFlag,currentSignal:currentSignal.signal,currentName:currentSignal.name}')
            stream-voice-curve(ng-if='chartModal == 1' controller='vm',parameters='{station:station.model.station,equipment:equipment.model.equipment,streamFlag:streamFlag,currentSignal:currentSignal.signal,currentName:currentSignal.name}')
        .original(style="display:block;height:100%;width:100%" ng-if="!originalDataFlag")
            stream-origin-current(controller='vm',parameters='{station:station.model.station,equipment:equipment.model.equipment,streamFlag:streamFlag,currentSignal:currentSignal.signal,currentName:currentSignal.name}')
            //stream-origin-voice(ng-if='chartModal == 1' controller='vm',parameters='{station:station.model.station,equipment:equipment.model.equipment,streamFlag:streamFlag,currentSignal:currentSignal.signal,currentName:currentSignal.name}')

